#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Blog/Blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Blog/Blog/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+jake@10.8.7/node_modules/jake/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+jake@10.8.7/node_modules/jake/bin/cli.js" "$@"
fi
