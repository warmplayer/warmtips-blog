#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/Blog/Blog/node_modules/.store/nunjucks@3.2.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/Blog/Blog/node_modules/.store/nunjucks@3.2.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nunjucks@3.2.4/node_modules/nunjucks/bin/precompile" "$@"
else
  exec node  "$basedir/../../../../../nunjucks@3.2.4/node_modules/nunjucks/bin/precompile" "$@"
fi
